{
  "name": "clabe45.github.io",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://clabe45.github.io",
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "bootstrap": "^4.5.2",
    "bootstrap-icons": "^1.0.0",
    "gh-pages": "^3.1.0",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-bootstrap-icons": "^1.0.0",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "showdown": "^1.9.1",
    "stylelint": "^13.7.1",
    "stylelint-config-standard": "^20.0.0"
  },
  "scripts": {
    "setup": "git submodule update --init --recursive",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "lint": "npx eslint src && npx stylelint src",
    "update": "git submodule foreach git pull origin master",
    "build": "rm -rf build && npm run update && react-scripts build && cd etro-js.github.io && npm i && npm run build && git restore package-lock.json && cd .. && node vidar-redirect/inject-redirects.js && cp -r etro-js.github.io/public build/vidar",
    "prepublish": "npm run build",
    "publish": "gh-pages -d build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-eslint": "^10.1.0",
    "cheerio": "^1.0.0-rc.10",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^2.5.1",
    "walk-sync": "^3.0.0"
  }
}
